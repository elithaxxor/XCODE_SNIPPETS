// MARK: Extensions for random Int and Double
extension Int {
    var randomInt: Int {
        if self &gt; 0 {
            return Int(arc4random_uniform(UInt32(self)))
        } else if self &lt; 0 {
            return -Int(arc4random_uniform(UInt32(abs(self))))
        } else {
            return 0
        }
    }
}
